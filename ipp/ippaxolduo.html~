<script>
	//referencia1
	function tooltip(){
		var verEstado  = $("#cap").css('opacity');
		var verEstado1  = $("#cap1").css('opacity');
		var verEstado2  = $("#cap2").css('opacity');
		var verEstado3  = $("#cap3").css('opacity');
		
		if(verEstado==0){
			$("#cap").stop().animate({height:'137px',opacity:1},{queue:false, duration:800});
		}else{
			$("#cap").stop().animate({height:'0px',opacity:0},{queue:false, duration:800});
		}
		if(verEstado1!=0){
			$("#cap1").stop().animate({height:'0px',opacity:0},{queue:false, duration:800});
		}
		if(verEstado2!=0){
			$("#cap2").stop().animate({height:'0px',opacity:0},{queue:false, duration:800});
		}
		if(verEstado3!=0){
			$("#cap3").stop().animate({height:'0px',opacity:0},{queue:false, duration:800});
		}

	}
	$("#cap").stop().animate({height:'0px',opacity:0},{queue:false, duration:0});
	document.getElementById('cap').style.display="";
	document.getElementById("cap").style.marginTop = "55px";
	document.getElementById("cap").style.marginLeft = "460px";
	document.getElementById("cap").style.position = "absolute";
	estilo=document.getElementById("cap").style.zIndex+1000
	document.getElementById("cap").style.zIndex=estilo;
	//fin referencia1
	
	//referencia2
	function tooltip1(){
		var verEstado  = $("#cap1").css('opacity');
		var verEstado1  = $("#cap").css('opacity');
		var verEstado2  = $("#cap2").css('opacity');
		var verEstado3  = $("#cap3").css('opacity');
		
		if(verEstado==0){
			$("#cap1").stop().animate({height:'137px',opacity:1},{queue:false, duration:800});
		}else{
			$("#cap1").stop().animate({height:'0px',opacity:0},{queue:false, duration:800});
		}
		if(verEstado1!=0){
			$("#cap").stop().animate({height:'0px',opacity:0},{queue:false, duration:800});
		}
		if(verEstado2!=0){
			$("#cap2").stop().animate({height:'0px',opacity:0},{queue:false, duration:800});
		}
		if(verEstado3!=0){
			$("#cap3").stop().animate({height:'0px',opacity:0},{queue:false, duration:800});
		}
	}
	$("#cap1").stop().animate({height:'0px',opacity:0},{queue:false, duration:0});
	document.getElementById('cap1').style.display="";
	document.getElementById("cap1").style.marginTop = "194px";
	document.getElementById("cap1").style.marginLeft = "495px";
	document.getElementById("cap1").style.position = "absolute";
	estilo=document.getElementById("cap1").style.zIndex+1000
	document.getElementById("cap1").style.zIndex=estilo;
	//fin referencia2
	
	
	//referencia 4
	function tooltip3(){
		var verEstado  = $("#cap3").css('opacity');
		var verEstado1  = $("#cap").css('opacity');
		var verEstado2  = $("#cap1").css('opacity');
		var verEstado3  = $("#cap2").css('opacity');
		
		if(verEstado==0){
			$("#cap3").stop().animate({height:'137px',opacity:1},{queue:false, duration:800});
		}else{
			$("#cap3").stop().animate({height:'0px',opacity:0},{queue:false, duration:800});
		}
		if(verEstado1!=0){
			$("#cap").stop().animate({height:'0px',opacity:0},{queue:false, duration:800});
		}
		if(verEstado2!=0){
			$("#cap1").stop().animate({height:'0px',opacity:0},{queue:false, duration:800});
		}
		if(verEstado3!=0){
			$("#cap2").stop().animate({height:'0px',opacity:0},{queue:false, duration:800});
		}
	}
	$("#cap3").stop().animate({height:'0px',opacity:0},{queue:false, duration:0});
	document.getElementById('cap3').style.display="";
	document.getElementById("cap3").style.marginTop = "335px";
	document.getElementById("cap3").style.marginLeft = "475px";
	document.getElementById("cap3").style.position = "absolute";
	estilo=document.getElementById("cap3").style.zIndex+903
	document.getElementById("cap3").style.zIndex=estilo;
	//fin referencia4
	
	function tooltip4(){
		var verEstado  = $("#cap4").css('opacity');
		
		
		if(verEstado==0){
			$("#cap4").stop().animate({height:'52px',opacity:1},{queue:false, duration:400});
		}else{
			$("#cap4").stop().animate({height:'0px',opacity:0},{queue:false, duration:400});
		}
		

	}
	$("#cap4").stop().animate({height:'52px',opacity:1},{queue:false, duration:0});
	document.getElementById('cap4').style.display="";
	document.getElementById("cap4").style.marginTop = "704px";
	document.getElementById("cap4").style.marginLeft = "0px";
	document.getElementById("cap4").style.position = "absolute";
	estilo=document.getElementById("cap4").style.zIndex+903
	document.getElementById("cap4").style.zIndex=estilo;
