	function tooltip4(){
		var verEstado  = $("#cap4").css('opacity');
		
		
		if(verEstado==0){
			$("#cap4").stop().animate({height:'52px',opacity:1},{queue:false, duration:400});
		}else{
			$("#cap4").stop().animate({height:'0px',opacity:0},{queue:false, duration:400});
		}
		

	}
	$("#cap4").stop().animate({height:'52px',opacity:1},{queue:false, duration:0});
	document.getElementById('cap4').style.display="";
	document.getElementById("cap4").style.marginTop = "704px";
	document.getElementById("cap4").style.marginLeft = "0px";
