	//referencia 4
	function tooltip3(){
		var verEstado  = $("#cap3").css('opacity');
		var verEstado1  = $("#cap").css('opacity');
		var verEstado2  = $("#cap1").css('opacity');
		var verEstado3  = $("#cap2").css('opacity');
		
		if(verEstado==0){
			$("#cap3").stop().animate({height:'137px',opacity:1},{queue:false, duration:800});
		}else{
			$("#cap3").stop().animate({height:'0px',opacity:0},{queue:false, duration:800});
		}
		if(verEstado1!=0){
			$("#cap").stop().animate({height:'0px',opacity:0},{queue:false, duration:800});
		}
		if(verEstado2!=0){
			$("#cap1").stop().animate({height:'0px',opacity:0},{queue:false, duration:800});
		}
		if(verEstado3!=0){
			$("#cap2").stop().animate({height:'0px',opacity:0},{queue:false, duration:800});
		}
	}
